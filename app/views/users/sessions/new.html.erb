<h2>ログイン</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, autofocus: true ,:autocomplete => "on"%>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password, autocomplete: "on" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <br>
    <div class="field">
      <%= f.check_box :remember_me %> ID、Passwordを記憶する
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in", :class => 'btn btn-default'%>
  </div>
<% end %>

<%= render "users/shared/links" %>
